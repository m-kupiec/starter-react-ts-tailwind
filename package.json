{
  "name": "starter-app-react-ts-tailwind",
  "private": true,
  "version": "1.0.37",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "types": "tsc --project tsconfig.app.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
